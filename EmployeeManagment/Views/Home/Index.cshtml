@model IEnumerable<EmployeeManagment.Models.Employee>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Department</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>@item.Id</tr>
                <tr>@item.Name</tr>
                <tr>@item.Department</tr>
            }
        </tbody>
    </table>
</body>
</html>
